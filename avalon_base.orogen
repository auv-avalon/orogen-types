name "avalon_base"
version "0.1"

using_library 'avalon_base-types'

# Common avalon types that are not opaques
import_types_from "avalon_base/control.h"



typekit do
    if type_export_policy == :used
        # We assume that, if the caller wants a 'used' type policy, it means he
        # wants partial exports. Move to 'selected'. oroGen would have changed
        # it to 'all'
        type_export_policy :selected
    end

	export_types
		'/avalon/DOFSelection'
	
end
